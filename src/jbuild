(library
 ((name        ppx_js_style)
  (public_name ppx_js_style)
  (kind ppx_rewriter)
  (flags (:standard -safe-string))
  (libraries (compiler-libs.common ppx_core ppx_driver octavius))
  (preprocess (pps (ppx_metaquot ppx_driver.runner)))))

(rule
